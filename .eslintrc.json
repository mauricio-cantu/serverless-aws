{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "error",
        "prefer-const": "error",
        "space-before-function-paren":"error",
        "space-before-blocks": "error",
        "no-trailing-spaces": "error",
        "space-in-parens": "error",
        "space-infix-ops": "error",
        "indent": ["error", 2]
    }
}
